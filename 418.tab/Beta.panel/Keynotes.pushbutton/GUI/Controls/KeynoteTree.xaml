<ResourceDictionary xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
                    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml">
    <ControlTemplate x:Key="KeynoteTreeTemplate" TargetType="ContentControl">
        <Border Style="{DynamicResource CardStyle}" Margin="0,0,6,0">
            <Grid>
                <Grid.RowDefinitions>
                    <RowDefinition Height="Auto"/>
                    <RowDefinition Height="*"/>
                </Grid.RowDefinitions>
                <TextBlock Text="Keynotes Tree" Style="{DynamicResource SubHeaderTextBlockStyle}" Margin="0,0,0,8"/>
                <TreeView Name="MainTreeView" Grid.Row="1" BorderThickness="0" Background="Transparent" ItemTemplate="{DynamicResource KeynoteItemTemplate}"/>
            </Grid>
        </Border>
    </ControlTemplate>
</ResourceDictionary>
